{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/affiliates_connect": "1.x-dev",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.3",
        "drupal/ctools": "^3.0",
        "drupal/entity_browser": "^2.0",
        "drupal/feeds": "^3.0",
        "drupal/feeds_ex": "^1.0",
        "drupal/feeds_tamper": "2.x-dev",
        "drupal/robotstxt": "^1.2",
        "drupal/search_api": "1.x-dev",
        "drupal/search_api_page": "1.x-dev",
        "drupal/entity_comparison": "1.x-dev",
        "drupal/feeds_advance_crawler": "1.x-dev",
        "drupal/tamper": "1.x-dev",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/affiliates_connect": {
                "#2983176": "https://www.drupal.org/files/issues/2018-08-06/2983176-6.patch",
                "#2985796": "https://www.drupal.org/files/issues/2018-07-25/2985796-11.patch",
                "#2987416": "https://www.drupal.org/files/issues/2018-08-09/2987416-13.patch",
                "#2990538": "https://www.drupal.org/files/issues/2018-08-09/2990538-7.patch",
                "#2991976": "https://www.drupal.org/files/issues/2018-08-10/2991976-2.patch"
            }
        }
    }
}
